{
  "name": "lab-javascript-chronometer",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll --verbose=false"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "jest-html-reporter": "^3.3.0",
    "jest-junit": "^12.0.0"
  },
  "jest": {
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Lab Solution",
          "outputPath": "lab-solution.html",
          "includeFailureMsg": true,
          "includeConsoleLog": true
        }
      ]
    ]
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "description": "![logo_ironhack_blue 7](https://user-images.githubusercontent.com/23629340/40541063-a07a0a8a-601a-11e8-91b5-2f13e4e6b441.png)",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafael-alcon/lab-javascript-chronometer.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/rafael-alcon/lab-javascript-chronometer/issues"
  },
  "homepage": "https://github.com/rafael-alcon/lab-javascript-chronometer#readme"
}
